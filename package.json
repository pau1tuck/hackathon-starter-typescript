{
    "name": "hackathon-starter-typescript",
    "version": "1.1.0",
    "description": "A boilerplate for Node.js web applications with TypeScript",
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/pau1tuck/hackathon-starter-typescript.git"
    },
    "license": "MIT",
    "author": "Sahat Yalkabov",
    "contributors": [
        "Paul D.R. Tuck (https://github.com/pau1tuck)",
        "Yashar Fakhari (https://github.com/YasharF)"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "concurrently \"tsc -w\" \"nodemon dist/app.js\"",
        "lint": "eslint \"**/*.ts\"",
        "start": "node dist/app.js",
        "test": "nyc mocha --timeout=10000 --exit",
        "watch": "tsc -w"
    },
    "dependencies": {
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^20.4.1",
        "chai": "^4.3.6",
        "concurrently": "^8.2.0",
        "dotenv": "^16.3.1",
        "eslint": "^8.40.0",
        "eslint-config-airbnb-typescript-prettier": "^5.0.0",
        "eslint-plugin-chai-friendly": "^0.7.2",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "mocha": "^9.2.2",
        "mongodb-memory-server": "^8.4.1",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.0",
        "ts-node": "^10.9.1",
        "tsc": "^2.0.4",
        "tslib": "^2.6.0",
        "typescript": "^5.1.6"
    },
    "engines": {
        "node": ">=18"
    }
}
